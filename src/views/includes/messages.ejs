<% if(errors.length > 0) { %>
<div class="row container mx-auto mb-3">
  <div class="col my-3 alert alert-danger">
    <% errors.forEach(erro => { %>
      <%= erro %> <br>
   <% }); %>
  </div>
</div>
<% } %>

<% if(success.length > 0) { %>
<div class="row container mx-auto mb-3">
  <div class="col my-3 alert alert-success">
    <% success.forEach(success => { %>
      <%= success %> <br>
   <% }); %>
  </div>
</div>
<% } %>